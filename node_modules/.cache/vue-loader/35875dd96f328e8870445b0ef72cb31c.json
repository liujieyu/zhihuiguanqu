:["<template>\r\n  <div>\r\n\t<Content class=\"searchcon\">\r\n      <Row type=\"flex\" :gutter=\"16\" class=\"rowtocol\">\r\n        <Col>\r\n          <Input search enter-button suffix=\"ios-search\" placeholder=\"请输入站名：\" style=\"width: auto;margin-right: 20px;\" @on-search=\"search\" v-model=\"form.searchmsg\" />\r\n        </Col>\r\n        <Col>\r\n          <Button type=\"primary\" style=\"width: auto;margin-right: 20px;\" @click=\"exportToExcel\">导出</Button>\r\n        </col>\r\n      </Row>\r\n      <Row class=\"fgline\"></Row>\r\n      <el-table\r\n        :data=\"tabledata\"\r\n        border\r\n        :height=\"theight\"\r\n        v-loading=\"loading\"\r\n        style=\"width: 100%\"\r\n        @cell-click=\"cellclick\"\r\n        @sort-change=\"sort_change\">\r\n        <el-table-column\r\n          label=\" \"\r\n          prop=\"ROWID\"\r\n          align=\"center\"\r\n          width=\"65\"\r\n          fixed=\"left\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"STNM\"\r\n           label=\"站名\"\r\n           align=\"center\"\r\n           fixed=\"left\"\r\n           sortable=\"custom\"\r\n           min-width=\"150\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"XHWL\"\r\n           label=\"校核洪水位\"\r\n           align=\"center\"\r\n           fixed=\"left\"\r\n           sortable=\"custom\"\r\n           min-width=\"120\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"ZCWL\"\r\n           label=\"正常蓄水位\"\r\n           align=\"center\"\r\n           fixed=\"left\"\r\n           sortable=\"custom\"\r\n           min-width=\"120\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"FWL\"\r\n           label=\"4-6月汛限水位\"\r\n           align=\"center\"\r\n           fixed=\"left\"\r\n           sortable=\"custom\"\r\n           min-width=\"140\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=