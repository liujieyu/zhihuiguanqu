nter\"\r\n           sortable=\"custom\"\r\n           min-width=\"140\">\r\n        </el-table-column>       \r\n        <el-table-column\r\n           prop=\"FWL\"\r\n           label=\"4-6月汛限水位(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"160\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"FWL79\"\r\n           label=\"7-9月汛限水位(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"160\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"ZCWL\"\r\n           label=\"正常蓄水位(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"140\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"SJWL\"\r\n           label=\"设计洪水位(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"140\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"actz\"\r\n           label=\"兴利水位(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"130\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"SWL\"\r\n           label=\"死水位(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"130\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"laz\"\r\n           label=\"低水位告警(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"145\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"hhrz\"\r\n           label=\"历史最高水位(m)\"\r\n           align=\"center\"\r\n           sortable=\"custom\"\r\n           min-width=\"155\">\r\n        </el-table-column>\r\n        <el-table-column\r\n           prop=\"stlc\"\r\n           label=\"所属行政区划\"\r\n           align=\"center\"\r\n           sortable=\"custom\"