                     <Col style=\"text-align: left\">\r\n                      总站数：{{Tables.total}}，单位：水位 m，流量m³/s，水量 10⁶m³\r\n                      </Col>\r\n                      <Col style=\"position: absolute;right: 0;bottom: 0px;\">\r\n                        <Button type=\"primary\" style=\"width: auto;margin-right: 20px;\" @click=\"err\">导出</Button>\r\n                      </Col>\r\n                    </Row>\r\n                    <Divider />\r\n                    <el-table\r\n                        :data=\"Tables.data\"\r\n                        border\r\n                        height=\"500\"\r\n                        v-loading=\"loading\"\r\n                        @sort-change=\"sort_change\"\r\n                        style=\"width: 100%\">\r\n                        <el-table-column\r\n                          prop=\"Canal_Name\"\r\n                          label=\"渠道\"\r\n                          sortable\r\n                          align=\"center\"\r\n                          width=\"100\"\r\n                          >\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                          prop=\"STNM\"\r\n                          label=\"站名\"\r\n                          sortable\r\n                          align=\"center\"\r\n                          width=\"\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                          prop=\"RZ\"\r\n                          label=\"平均水位\"\r\n                          sortable\r\n                          align=\"center\"\r\n                          width=\"140\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                          prop=\"INQ\"\r\n                          label=\"平均入库流量\"\r\n                          sortable\r\n                          align=\"center\"\r\n                          width=\"140\"\r\n                          >