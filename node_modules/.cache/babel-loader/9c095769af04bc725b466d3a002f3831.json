: [],\r\n                    Field: '',\r\n                },\r\n                csjg: {\r\n                    FlumBody_Type: [],\r\n                    Field: '',\r\n                },\r\n                list_input:{\r\n                    total:100,\r\n                    pagesize:50,\r\n                    pagesizeopts:[10,20,50,75,100,200],\r\n                    current:1,\r\n                },\r\n            }\r\n        },\r\n                // 引入过滤方法到此组件\r\n        mixins: [FilterMethods,GetDataMethods],\r\n        mounted(){\r\n            this.Reload();\r\n        },\r\n        methods:{\r\n            err(){\r\n              // console.log(\"showDrawer\",this.$showDrawer);\r\n                this.$Message.warning('该功能还在开发中...');\r\n            },\r\n            search(){\r\n              if (this.form.searchmsg != '') {\r\n                this.searchs = '&Flum_Name=like,'+this.form.searchmsg;\r\n              }else{\r\n                this.searchs = '';\r\n              }\r\n                this.Reload();\r\n            },\r\n            indexMethod(index){\r\n                return index + 1 + (this.list_input.pagesize*(this.list_input.current-1));\r\n            },\r\n            timechange(date){\r\n                console.log(date);\r\n                this.date = date;\r\n            },\r\n            Reload(){\r\n                // this.loading = true;\r\n                this.axios.get('/guanqu/admin/WRP_IrrBTFlume?_page_size='+this.list_input.pagesize+'&_page='+this.list_input.current+this.searchs,{params:{Matieral_Type: this.cllx.Field, FlumBody_Type: this.csjg.Field,}}).then((res)=>{\r\n                    console.log(res);\r\n                    this.loading = false;\r\n                    this.data = res.data.rows;\r\n                    this.list_input.total = res.data.total;\r\n                    for (var i = 0;i<this.data.length ; i++) {\r\n                      this.data[i].D_FL = this.Z_Filter(this.data[i].D_FL,3);\r\n                      this.data[i].ADD_FL = this.Z_Filter(this.data[i].ADD_FL,3);\r\n                      this.data[i].IN_Z = this.Z_Filter(this.data[i].IN_Z);\r\n                      this.data[i].OUT_Z = this.Z_Filter(this.data[i].OUT_Z);\r\n                    }\r\n                });\r\n                this.getTableData_WRP_IrrBTCanalSystem(data => {\r\n                  this.form.qudaoList = data;\r\n                });\r\n                this.axios.get('/guanqu/admin/WRP_FieldInfo?FieldID=Matieral_Type').then((res)=>{\r\n                  this.cllx.Matieral_Type = res.data.rows;\r\n                });\r\n                this.axios.get('/guanqu/admin/WRP_FieldInfo?FieldID=FlumBody_Type').then((res)=>{\r\n                  this.csjg.FlumBody_Type = res.data.rows;\r\n                });\r\n            },\r\n            // 处理页码切换\r\n            CurrentChange(index) {\r\n                // console.log(index);\r\n              this.list_input.current = index;\r\n              this.Reload();\r\n            },\r\n            // 处理每页显示条数切换\r\n            PagesizeChange(pagesize) {\r\n                // console.log(pagesize)\r\n              this.list_input.pagesize = pagesize;\r\n              this.Reload();\r\n            },\r\n        },\r\n    }\r\n</script>\r\n\r\n<style type=\"text/css\">\r\n\r\n</style>\r\n"],"sourceRoot":"src/table/table-basedata/table-jzw"}]}