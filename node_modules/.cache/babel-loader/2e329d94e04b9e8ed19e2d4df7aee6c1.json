                    :data=\"data1\"\r\n                            border\r\n                            height=\"510\"\r\n                            v-loading=\"loading\"\r\n                            @cell-click=\"cellclick\"\r\n                            @sort-change=\"sort_change\"\r\n                            style=\"width: 500px\">\r\n                            <el-table-column\r\n                              label=\" \"\r\n                              type=\"index\"\r\n                              align=\"center\"\r\n                              width=\"60\"\r\n                              :index=\"indexMethod\">\r\n                            </el-table-column>\r\n                            <el-table-column\r\n                              prop=\"tm\"\r\n                              label=\"时间\"\r\n                              sortable=\"custom\"\r\n                              align=\"center\"\r\n                              width=\"\">\r\n                            </el-table-column>\r\n                            <el-table-column\r\n                              prop=\"Z\"\r\n                              label=\"水深\"\r\n                              sortable=\"custom\"\r\n                              align=\"center\"\r\n                              width=\"80\"\r\n                              >\r\n                            </el-table-column>\r\n                            <el-table-column\r\n                              prop=\"Q\"\r\n                              label=\"流量\"\r\n                              sortable=\"custom\"\r\n                              align=\"center\"\r\n                              width=\"80\"\r\n                              >\r\n                            </el-table-column>\r\n                          </el-table>\r\n              <div style=\"margin: 10px;overflow: hidden\">\r\n                <div style=\"float: right;\">\r\n                  <Page\r\n                    :total=\"list_input.total\"